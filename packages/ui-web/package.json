{
  "name": "@buskerone/ui-web",
  "version": "0.0.0",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/styles/tokens.css"
  ],
  "peerDependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "dependencies": {
    "@buskerone/primitives": "workspace:*",
    "@buskerone/tokens": "workspace:*"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/react": "19.2.7",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
